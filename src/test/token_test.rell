@test module;
import ^^.user.{user, create_user};
import ^^.token.{token, mint_token, switch_favorite_token, transfer_token};

function test_mint_token() {

    val alice_pk = rell.test.keypairs.alice.pub;
    val alice_un = 'Alice';
    val token_name = "Tokenname";
    val token_text = "BestToken";

    rell.test.tx(create_user(alice_pk, alice_un)).run();
    rell.test.tx(mint_token(alice_pk, token_text, token_name)).run();
    
    val t1 = token@{};
    print('name1----', t1, t1.name);

    val t2 = token@{.name == token_name};
    print('name2----', t2);
}
