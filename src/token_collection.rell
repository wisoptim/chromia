module;
import user.*;
import token.*;

entity token_collection { 
  key name;
  key owner:user;
  mutable token: token;
}

operation create_collection (collection_name:name, token_name:name) {
  val pubkey = get_signer_pkey();
  val owner = user@{pubkey};

  val token = token@{.name == token_name};
  create token_collection (collection_name, owner, token);
}
